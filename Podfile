platform :ios, '13.0'
use_frameworks!


def shared_pods
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'GoogleSignIn'
  pod 'NotionSwift', '0.5.1'
  pod 'SwiftSoup'
  pod 'SDWebImage'
end

target 'BookMarksApp' do
  shared_pods
end
